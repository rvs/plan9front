link
#	apm		apmjump

# order of ethernet drivers should match that in ../pcboot/boot so that
# devices are detected in the same order by bootstraps and kernels
# and thus given the same controller numbers.  put fastest first.
# 10 Gb/s
	etherxgbe	pci
# gigabit
	etherigbepci	pci ethermii
	etherigbe	pci
	ether8169	pci ethermii
# 100 Mb/s
	ether82557	pci
	ether8139	pci
	etherelnk3	pci
	ethervt6105m	pci ethermii
# dog slow, mainly for use in virtual machines
	ether2000	ether8390
	ether79c970	pci
	ether8003	ether8390

	ethermedium
	netdevmedium

#	usbuhci
#	usbohci
	usbehci		usbehcipc

# only needed for vga
	realmode

misc
	mp		apic mpacpi
#	nomp

	sdata		pci sdscsi
	sd53c8xx	pci sdscsi
	sdmylex		pci sdscsi
	sdahci		pci sdscsi
	sdnvme		pci sdscsi

	uarti8250
#	uartpci		pci
#	uartisa

	vgai81x		+cur
	vganvidia	+cur
	vgaradeon	+cur
	vgargb524	=cur
	vgas3		+cur vgasavage
	vgat2r4		+cur
	vgavesa		realmode
	vgavmware	+cur

ip
	tcp
	udp
	ipifc
	icmp
	icmp6

port
	int cpuserver = 0;

boot boot #S/sdD0/data
	tcp
	local

